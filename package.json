    },
    "es5-ext": "0.10.63",
    "fast-glob": {
      "braces": "3.0.3"
    },
    "follow-redirects": "1.15.6",
    "ip": "2.0.1",
    "jsdom": "16.5.0",
    "oldie": {
      "postcss": "$postcss"
    },
    "pixrem": {
      "postcss": "^7.0.36"
    },
    "postcss-calc": {
      "postcss": "$postcss"
    },
    "postcss-pseudoelements": {
      "postcss": "$postcss"
    },
    "tar": "6.2.1",
    "tough-cookie": "4.1.3",
    "ws": "6.2.3"
  }
}
