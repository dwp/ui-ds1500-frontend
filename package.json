{
  "name": "ds1500-frontend",
  "version": "2.4.0",
  "description": "Citizen-facing CASA application",
  "license": "ISC",
  "author": "",
  "main": "app.js",
  "scripts": {
    "build": "gulp && npm run build:js && npm run build:html5shiv && npm run build:scss && npm run build:scss-ie8 && npm run build:pages",
    "build:html5shiv": "cp ./node_modules/html5shiv/dist/html5shiv.min.js ./static/js",
    "build:js": "rollup -c",
    "build:pages": "env $(cat .env.example | grep -v ^# | xargs) gulp staticPages",
    "build:scss": "sass assets/all.scss -I node_modules --no-source-map | postcss -o static/css/ui-ds1500-frontend.css",
    "build:scss-ie8": "sass assets/all-ie8.scss -I node_modules --no-source-map | IE8=true postcss -o static/css/ui-ds1500-frontend-ie8.css",
    "postinstall": "npm run build",
    "quality:coverage": "./node_modules/istanbul/lib/cli.js cover _mocha -- -R spec test/unit**/*",
    "quality:lint": "eslint .",
    "quality:sonarqube": "sonar-scanner",
    "security:vulnerable-packages": "npm audit --registry=https://registry.npmjs.org --json | node -e 'a=JSON.parse(fs.readFileSync(\"/dev/stdin\",\"utf-8\")).metadata.vulnerabilities;process.exit(a.high+a.critical);'",
    "start": "node .",
    "start-background": "node . &",
    "start-mock-service": "node ./tools/fake-service.js &",
    "start:debug": "node --inspect .",
    "start:debugWatch": "nodemon --inspect .",
    "start:dev": "env $(cat .env.example | grep -v ^# | xargs) npm run start:debugWatch",
    "stop-background": "lsof -ti:3000 | xargs kill",
    "stop-mock-service": "lsof -ti:9013 | xargs kill",
    "test": "npm run test:unit",
    "test:integration": "mocha 'test/integration/**/*.tests.js'",
    "test:standalone": "npm run start-background && npm run start-mock-service && ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/**/*.tests.js && npm run stop-background && npm run stop-mock-service",
    "test:unit": "mocha test/unit**/**/*",
    "withEnv": "env $(cat .env.example | grep -v ^# | xargs) $@"
  },
  "dependencies": {
    "@dwp/govuk-casa": "8.15.2",
    "@dwp/node-logger": "2.0.2",
    "bunyan": "1.8.15",
    "bunyan-syslog-udp": "0.2.0",
    "config": "3.3.12",
    "connect-redis": "7.1.1",
    "cookie-parser": "1.4.6",
    "easy-autocomplete": "1.3.5",
    "express": "4.21.0",
    "express-session": "1.18.0",
    "google-libphonenumber": "3.2.38",
    "govuk_frontend_toolkit": "9.0.1",
    "govuk_template_jinja": "0.26.0",
    "govuk-elements-sass": "3.1.3",
    "govuk-frontend": "4.7.0",
    "gulp-dart-sass": "1.1.0",
    "gulp-jsonmin": "1.2.0",
    "gulp-rename": "2.0.0",
    "gulp-uglify": "3.0.2",
    "hmrc-frontend": "5.62.0",
    "ioredis": "5.4.1",
    "jquery": "3.7.1",
    "luxon": "3.5.0",
    "memorystore": "1.6.7",
    "minimatch": "10.0.1",
    "node-gyp": "10.2.0",
    "notifications-node-client": "8.2.1",
    "num2fraction": "1.2.2",
    "parsleyjs": "2.9.2",
    "pump": "3.0.2",
    "request": "2.88.2",
    "request-promise-native": "1.0.9",
    "run-sequence": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@babel/eslint-parser": "7.11.0",
    "@babel/preset-env": "7.22.20",
    "@rollup/plugin-alias": "5.0.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-legacy": "3.0.2",
    "@rollup/plugin-node-resolve": "15.2.2",
    "@rollup/plugin-terser": "0.4.4",
    "autoprefixer": "10.4.16",
    "chai": "4.3.10",
    "chai-as-promised": "7.1.1",
    "chai-http": "4.3.0",
    "eslint": "8.50.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-standard": "5.0.0",
    "gulp": "4.0.2",
    "hmrc-frontend-src": "https://github.com/hmrc/hmrc-frontend.git#v5.62.0",
    "gulp-concat": "^2.6.1",
    "html5shiv": "3.7.3",
    "istanbul": "0.4.5",
    "mocha": "10.2.0",
    "nan": "2.18.0",
    "nodemon": "3.0.1",
    "oldie": "1.3.0",
    "postcss": "^8.4.39",
    "postcss-cli": "10.1.0",
    "postcss-url": "10.1.3",
    "rewire": "7.0.0",
    "rollup": "4.0.0",
    "sass": "1.69.0",
    "sinon": "16.1.0",
    "sinon-chai": "3.7.0",
    "zombie": "6.1.4"
  },
  "overrides": {
